{
    "name": "hexlet/code",
    "bin": [
        "bin/php-package"
    ],
    "type": "project",
    "authors": [
        {
            "name": "Nikita Krochak",
            "email": "krochak_n@mail.ru"
        }
    ],
    "description": "Page Loader",
    "license": "MIT",
    "scripts": {
        "test": "phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "Hexlet\\PageLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hexlet\\PageLoader\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "hexlet/phpstan-fp": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.98",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpunit/phpunit": "^9.5",
        "psy/psysh": "@stable",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "require": {
        "php": "^8.0",
        "docopt/docopt": "^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.60",
        "imangazaliev/didom": "^1.18",
        "mikey179/vfsstream": "^1.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "beta"
}
